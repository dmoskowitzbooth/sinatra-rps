<!-- /views/paper.erb -->

<!DOCTYPE html>
<html>
<head>
  <title>Paper</title>
</head>

<body>

<h1>We played paper!</h1>
  <p><%= @outcome %></p>

  <% if @play == "paper"%>
   <p>We tied!</p>
  <% elsif @play == "rock"%>
   <p>We won!</p>
  <% else %>
   <p>We lost!</p>
  <% end %>
</body>
</html>
